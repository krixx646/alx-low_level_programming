#include <stdio.h>

/*
 * main:  This program writes a password "Hol" to a file named "101-password".
 * If the file cannot be opened for writing, it prints an error message.
 */


int main(void) {
    FILE *fp;
    char *pass = "Hol";

    fp = fopen("101-password", "w");

    if (fp == NULL) {
        perror("Error opening file");
        return -1;
    }

    fputs(pass, fp);

    fclose(fp);

    return 0;
}


